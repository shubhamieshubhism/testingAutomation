<suite name ="CrossBrowserTesting" parallel = "tests" thread-count = "3">
    <test name ="Chrome Testing" >
        <parameter name = "browser" value="chrome">
            <classes>
                <class name="SeleniumLogics.CrossBrowserTesting"></class>
            </classes>
        </parameter>
    </test>

    <test name ="Firefox Testing" >
        <parameter name = "browser" value="firefox">
            <classes>
                <class name="SeleniumLogics.CrossBrowserTesting"></class>
            </classes>
        </parameter>
    </test>

    <test name ="Edge Testing" >
        <parameter name = "browser" value="edge">
            <classes>
                <class name="SeleniumLogics.CrossBrowserTesting"></class>
            </classes>
        </parameter>
    </test>
</suite>